{
	"name": "vscode-jsonp",
	"displayName": "vscode-jsonp",
	"description": "",
	"version": "0.0.2",
	"icon": "res/icon.png",
	"publisher": "luyizhi",
	"repository": {
		"type": "git",
		"url": "https://github.com/kangkang520/vscode-jsonp.git"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Programming Languages",
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:jsonp"
	],
	"keywords": [
		"jsonp",
		"json",
		"jsonc",
		"prefix",
		"yizhi",
		"prefix-json"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "jsonp",
				"aliases": [
					"JSON with Prefix"
				],
				"extensions": [
					".jsonp"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "jsonp",
				"scopeName": "source.json.prefix",
				"path": "./syntaxes/jsonp.tmGrammar.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}